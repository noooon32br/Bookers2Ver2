<h2>Follower Users</h2>
  <% if @users.exists? %>
      <% @users.each do |user| %>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image, size:'100x100' %></td>
      <td><%= user.name %></td>
      <td>フォロー数: <%= user.followings.count %></td>
      <td><%= link_to 'Show', user %></td>
      <% end %>
    <% else %>
      <p>ユーザーはいません</p>
    <% end %>
    </tr>
  </tbody>
</table>

